      <div class="form">
<form action="/update-user" method="POST">
    <fieldset>
    <legend> Update USER</legend>

    <div class="com_input dnone">
    <label for="">Id:</label>
    <input type="text" name="userId" value="<%= userEdit.id %>">
    </div>

    <div class="com_input">
    <label for="">MAIL</label>
    <input type="text" name="email" value="<%= userEdit.email %>">
    </div>

    <div class="com_input">
        <label for="">name</label>
        <input type="text" name="name" value="<%= userEdit.name %>">
    </div>

    <div class="com_input">
        <label for="">pasword</label>
        <input type="text" name="password" value="<%= userEdit.password %>">
    </div>
    <div>
        <button>SAVE</button>
    </div>
    </fieldset>
</form>
      </div>
